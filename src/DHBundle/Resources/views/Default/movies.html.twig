{% extends '::base.html.twig' %}

{% block content %}

	<div class="titulo">{% trans from "menu" into _locale %} STR_MOVIES {% endtrans %}</div>

	<ul>
		{% for pelicula in peliculas %}
			
			<li>{{ pelicula.fecha|date('d/m/Y') }} <a href="{{ url('dh_fullmovie',{'id' : pelicula.id, 'titulo' : pelicula.titulo|replace({' ':'-'}) } ) }}">{{ pelicula.titulo }}</a></li>
		
		{% endfor %}
	</ul>

{% endblock %}